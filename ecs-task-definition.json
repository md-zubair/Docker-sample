{
  "family": "jenkins-ecs",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::609245952838:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::your-account-id:role/your-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "webapp",
      "image": "REPLACE_WITH_IMAGE_URI",  // This will be replaced in your pipeline
      "memory": 512,
      "cpu": 500,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/webapp",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
